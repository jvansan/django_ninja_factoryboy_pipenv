alias dtest='pipenv run python manage.py test --keepdb'
alias dbtest='pipenv run python manage.py test'
alias dshell='pipenv run python manage.py shell'
alias 'gitforce'='git push && git pull'
alias 'start'='pipenv run python manage.py runserver'
alias 'migrate'='pipenv run python manage.py migrate'
alias 'mmigrate'='pipenv run python manage.py makemigrations'
alias 'init-migrate'='python manage.py migrate --fake'
alias 'envupdate'='pipenv install'
